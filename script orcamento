function clica(x){
       var altimg = $(x).attr("alt");
       var img="";
       var titulo="";
       switch (altimg) {
           case 'caricaturas':
                img = "imgs/noivinhos.png";
                titulo="Caricaturas";
               break;
            case 'miniaturas':
                img= "imgs/miniatura.png";
                titulo= "Miniaturas";
               break;
            case 'noivinhos':
                img = "imgs/noivinhos.png";
                titulo= "Noivinhos";
               break;
       }
        $.get("/orcamento.html",function(data){
            var form = document.querySelector("#teste");
            form.innerHTML = data;
        });
}
